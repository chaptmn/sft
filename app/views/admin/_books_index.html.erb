
<table class="admin_books">
  <thead>
    <th>タイトル</th>
    <th>著者</th>
    <th>値段</th>
    <th>個数</th>
  </thead>
  <tbody>
  <% books.each do |book| %>
  <tr>
    <td><%= link_to book.title, admin_book_path(book.id) %></td>
    <td><%= book.author %></td>
    <td><%= book.price %></td>
    <td><%= book.count %></td>
  </tr>
  <% end %>
 </tbody>
</table>
