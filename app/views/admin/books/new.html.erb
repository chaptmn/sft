
<ul class="book_new_form">
  <%= form_for @admin_book, url: {action: "create"} do |f| %>
    <li><%= f.label :title, "タイトル" %>
        <%= f.text_field :title %></li>
  <%= f.label :image, "画像" %> <%= f.file_field :image %> 
  <li><%= f.label :author, "著者" %><%= f.text_field :author %></li>
  <li><%= f.label :price, "値段" %><%= f.text_field :price %></li>
  <li><%= f.label :category_id, "カテゴリー" %><%= f.select :category_id , Category.all_ary%></li>
  <li><%= f.label :count, "個数" %><%= f.text_field :count %></li>
  <li><%= f.submit %></li>
<% end %>
</ul>
